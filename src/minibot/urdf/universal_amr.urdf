    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/lidar.STL" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.03 0 0.0217" rpy="0 0 0" />
      <mass value="0.1653" />
      <inertia ixx="0.1" ixy="0.1" ixz="0.1" iyy="0.1" iyz="0.1" izz="0.0001" />
    </inertial>
  </link>

  <joint name="lidar_joint" type="fixed">
