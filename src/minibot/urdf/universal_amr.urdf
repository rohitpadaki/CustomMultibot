<?xml version="1.0" ?>
<robot name="universal_amr"  xmlns:xacro="http://ros.org/wiki/xacro">

  <material name="White">
      <color rgba="1 1 1 1"/>
  </material>

  <material name="Orange">
      <color rgba="1 0.3 0.1 1"/>
  </material>

  <material name="Blue">
      <color rgba="0.2 0.2 1 1"/>
  </material>

  <material name="Black">
      <color rgba="0 0 0 1"/>
  </material>

  <link name="dummy_link">
    
  </link>

  <joint name="dummy_joint" type="fixed">
    <parent link="dummy_link"/>
    <child link="base_link"/>
  </joint>  

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/base_link.STL" />
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/base_link.STL"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.1296 0.0014 0.1036" rpy="0 0 0" />
      <mass value="0.8044" />
      <inertia ixx="0.0026" ixy="0.1" ixz="0.1" iyy="0.0015" iyz="0.1" izz="0.0041" />
    </inertial>
  </link>

  <link name="right_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/right_wheel.STL" />
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/right_wheel.STL" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0.0035 0" rpy="0 0 0" />
      <mass value="0.1036" />
      <inertia ixx="0.1" ixy="0.1" ixz="0.1" iyy="0.0002" iyz="0.1" izz="0.1" />
    </inertial>
  </link>

  <joint name="right_wheel_joint" type="continuous">
    <origin xyz="0.1005 -0.1852 0.0495" py="0 0 0" />
    <parent link="base_link" />
    <child link="right_wheel" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="left_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/left_wheel.STL" />
      </geometry>
      <material name="">
        <color rgba="0.7960 0.8235 0.9372 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/left_wheel.STL" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-1.5339 -0.0034 -8.1362" rpy="0 0 0" />
      <mass value="0.1036" />
      <inertia ixx="0.1" ixy="-0.1" ixz="0.1" iyy="0.0002" iyz="0.1" izz="0.1" />
    </inertial>
  </link>

  <joint name="left_wheel_joint" type="continuous">
    <origin xyz="0.1016 0.18525 0.0495" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="left_wheel" />
    <axis xyz="0 -1 0" />
  </joint>

  <link name="castor_wheel1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/castor_wheel1.STL" />
      </geometry>
      <material name="White" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$find(minibot)/models/castor_wheel1.STL" />
      </geometry>
    </collision>zzzz
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.0742" />
      <inertia ixx="0.1" ixy="0.1" ixz="0.1" iyy="0.1" iyz="0.1" izz="0.1" />
    </inertial>
  </link>

  <joint name="castor_wheel1_joint" type="fixed">
    <origin xyz="0.2929 -0.0080 0.04999" rpy="0 0 0" />
